                                        </li>
                                        <li>
                                            <div class="text-tiny">Track Order</div>
                                        </li>
                                    </ul>
                                </div>
                                <!-- order-track -->
                                <div class="wg-box mb-20">
                                    <div class="order-track">
                                        <div class="image">
                                            <img src="images/images-section/track-oder-1.png" alt="">
                                        </div>
                                        <div class="content">
                                            <h5 class="mb-20">Order Tracking</h5>
                                            <div class="infor mb-10">
                                                <div class="body-text">Order ID</div>
                                                <div class="body-title-2">Order #<%= order._id %></div>
                                            </div>
                                            
                                            <div class="infor mb-10">
                                                <div class="body-text">Order Placed:</div>
                                                <div class="body-title-2"><%= order.created_at %></div>
                                            </div>
                                            <div class="infor mb-20">
                                                <div class="body-text">Total Price:</div>
                                                <div class="body-title-2">â‚¹ <%= order.total_amount %></div>
                                            </div>
                                            <div class="infor mb-20">
                                                <div class="body-text">Shipping Address:</div>
                                                <div class="body-title-2"><%= order.shipping_address %></div>
                                            </div>
                                            <div class="infor mb-20">
                                                <div class="body-text">Payment Status:</div>
                                                <div class="body-title-2"><%= order.payment_status %></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- /order-track -->
                                <!-- detail -->
                                <div class="wg-box mb-20">
                                    <div>
                                        <h6 class="mb-10">Detail</h6>
                                        <div class="body-text">Your item is on the way. Tracking information will be available within 24 hours.</div>
                                    </div>
                                    <div class="road-map">
                                        <div class="road-map-item <%= order.status === 'receiving_orders' || order.status === 'order_processing' || order.status === 'being_delivered' || order.status === 'delivered' ? 'active' : '' %>">
                                            <div class="icon"><i class="<%= order.status === 'receiving_orders' || order.status === 'order_processing' || order.status === 'being_delivered' || order.status === 'delivered' ? 'icon-check' : 'icon-clock' %>"></i></div>
                                            <h6>Receiving orders</h6>
                                            <!-- <div class="body-text">05:43 AM</div> -->
                                        </div>
                                        <div class="road-map-item <%= order.status === 'order_processing' || order.status === 'being_delivered' || order.status === 'delivered' ? 'active' : '' %>">
                                            <div class="icon"><i class="<%= order.status === 'order_processing' || order.status === 'being_delivered' || order.status === 'delivered' ? 'icon-check' : 'icon-clock' %>"></i></div>
                                            <h6>Order processing</h6>
                                            <!-- <div class="body-text">01:21 PM</div> -->
                                        </div>
                                        <div class="road-map-item <%= order.status === 'being_delivered' || order.status === 'delivered' ? 'active' : '' %>">
                                            <div class="icon"><i class="<%= order.status === 'being_delivered' || order.status === 'delivered' ? 'icon-check' : 'icon-clock' %>"></i></div>
                                            <h6>Being delivered</h6>
                                            <!-- <div class="body-text"><%= order.status %></div> -->
                                        </div>
                                        <div class="road-map-item <%= order.status === 'delivered' ? 'active' : '' %>">
                                            <div class="icon"><i class="<%= order.status === 'delivered' ? 'icon-check' : 'icon-clock' %>"></i></div>
                                            <h6>Delivered</h6>
                                            <!-- <div class="body-text">Pending</div> -->
                                        </div>
                                        
                                        
                                       
                                    </div>
                                </div>
                                
                                
                                
